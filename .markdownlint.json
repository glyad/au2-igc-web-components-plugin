{
  "code-block-style": {
    "style": "fenced"
  },
  "code-fence-style": {
    "style": "backtick"
  },
  "emphasis-style": {
    "style": "asterisk"
  },
  "fenced-code-language": {
    "allowed_languages": [
      "ini",
      "json",
      "jsonc",
      "markdown",
      "bash",
      "shell",
      "sh",
      "javascript",
      "js",
      "typescript",
      "ts",
      "html",
      "css",
      "scss"
    ],
    "language_only": true
  },
  "heading-style": {
    "style": "atx"
  },
  "hr-style": {
    "style": "---"
  },
  "line-length": false,
  "link-image-style": {
    "autolink": false,
    "collapsed": false,
    "shortcut": false,
    "url_inline": false
  },
  "no-duplicate-heading": {
    "siblings_only": true
  },
  "ol-prefix": {
    "style": "one_or_ordered"
  },
  "MD029": false,
  "MD031": {
    "list_items": true
  },
  "proper-names": {
    "code_blocks": false,
    "names": [
      "CommonMark",
      "Ctrl",
      "JavaScript",
      "Markdown",
      "markdown-it",
      "markdownlint",
      "Node.js",
      "Shift",
      "Visual Studio Code"
    ]
  },
  "required-headings": {},
  "strong-style": {
    "style": "asterisk"
  },
  "table-pipe-style": {
    "style": "leading_and_trailing"
  },
  "ul-style": {
    "style": "dash"
  }
}
